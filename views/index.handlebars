<main id="main">

  <section id="facts">

    <div class="container wow fadeIn">
      {{#each restData}}
      {{this.REST_ID}}
      <div class="row">
        <div class="col-lg-3 col-6">
          <img src={{this.REST_IMAGE}} class="img-thumbnail" alt="">
        </div>
        <div class="col-lg-9 col-18">
          <div class="section-header">
            <h3 class="section-title"></h3>
            <p class="section-description">{{this.REST_ADDRESS}}</p>
          </div>
          <div class="row counters">


            <div class="col-lg-3 col-6 text-center">
              <span data-toggle="counter-up">{{this.REST_NAME}}</span>
              <p>Now Waiting</p>
            </div>

            <div class="col-lg-3 col-6 text-center">
              <span data-toggle="counter-up">

                {{#if this.checkins}}
                {{#each this.checkins}}
                {{#if @last}}
                {{this.CURRENT_WAIT}}
                {{/if}}
                {{/each}}
                {{else}}
                0
                {{/if}}
              </span>
              <p>Wait Time</p>
            </div>

            <div class="col-lg-3 col-6 text-center">
              <span data-toggle="counter-up">
                {{#if this.checkins.CURRENT_WAIT}}
                {{this.checkins.CURRENT_WAIT}}
                {{else}}
                0
                {{/if}}
              </span>
              {{!-- <p>Mins Wait (3 or more)</p> --}}
            </div>
            <button class="btn btn-primary myBtn" data-id={{this.REST_ID}}>Check into Waitlist</button>
          </div>
          <hr>

        </div>
      </div>

      {{/each}}
      <div id="myModal" class="modal" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
          <div class="modal-content">
            <div id="modelInputs input-group mb-3">
              <form class="clearfix mb-4 form-control" id="modalForm" action="/api/checkins/wdsdsadadas" method="POST">
                <h2>Add your current wait time!</h2>
                <input name="USER_EMAIL" type="email" class="form-control-1" placeholder="Enter Email Address">
                {{!-- <input name="REST_ID" type="hidden" id="example-text" class="form-control-1" placeholder="Enter Resturant"> --}}
                <input name="CURRENT_WAIT" type="number" class="form-control-1"
                  placeholder="Enter Current Wait Time (mins)">
                {{!-- <input name="PARTY_SIZE" type="number" class="form-control-1" placeholder="Enter Party Size"> --}}

                <button id="submitWait" class="btn btn-primary float-right">Submit</button>
              </form>
            </div>
          </div>
        </div>
      </div>

  </section><!-- #restaurants -->
